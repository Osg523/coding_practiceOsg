https://school.programmers.co.kr/learn/courses/30/lessons/340203

def solution(cpr):
    answer = []
    basic_order = ["check", "call", "pressure", "respiration", "repeat"]
    for action in 
cpr
:
        for i in 
range(len(basic_order)) 
:
            if action == basic_order[i]:
                answer.append(
(basic_order.index(basic_order[i])+1)
)
    return answer
